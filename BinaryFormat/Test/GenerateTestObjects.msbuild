<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="3.5" DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <UsingTask TaskName="More.BinaryFormat.BinaryFormatCodeGeneration" AssemblyFile="$(MSBuildProjectDirectory)\..\CodeGenerationTask\bin\Debug\More.BinaryFormat.CodeGenerationTask.dll" />
  <PropertyGroup>
    <Namespace>More.Net.BinaryFormatTestObjects</Namespace>
    <OutputFile>AutogeneratedTestObjects.cs</OutputFile>
    <ForceCodeGeneration>false</ForceCodeGeneration>
  </PropertyGroup>
  <ItemGroup>
    <InputFiles Include="TestObjects.binaryformat" />
  </ItemGroup>
  <Target Name="Build">
    <BinaryFormatCodeGeneration InputFiles="@(InputFiles)" OutputFile="$(OutputFile)"
                       Namespace="$(Namespace)" ForceCodeGeneration="$(ForceCodeGeneration)"/>
  </Target>
</Project>